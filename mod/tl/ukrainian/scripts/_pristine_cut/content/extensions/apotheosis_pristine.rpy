    # voice "audio/_pristine/voice/apotheosis/broken/18.flac"
    voice "audio/_pristine/voice/apotheosis/broken/18.flac"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:697
translate ukrainian apotheosis_tendrils_shared_join_d8d33007:

    # broken "It's gone. No... no! How do we get it back? Can we try again? We'll suffer so much better if we get to try again, I promise!\n"
    broken "Воно зникло. Ні... ні! Як нам його повернути? Може, спробуємо ще раз? Ми будемо страждати набагато краще, якщо спробуємо ще раз, я обіцяю!\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:698
