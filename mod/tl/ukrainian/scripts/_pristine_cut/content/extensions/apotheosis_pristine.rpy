# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:1073
translate ukrainian apotheosis_alone_final_join_b4470e0e:

    # sp "I forgive you your transgressions. But my destiny is inescapable.\n"
    sp "Я пробачаю твої гріхи. Але моя доля неминуча.\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:1074
