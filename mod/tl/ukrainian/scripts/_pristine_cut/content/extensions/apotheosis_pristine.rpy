    # voice "audio/_pristine/voice/apotheosis/princess/12a.flac"
    voice "audio/_pristine/voice/apotheosis/princess/12a.flac"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:560
translate ukrainian apotheosis_freedom_second_join_4f5e55d7:

    # sp "The flame may be snuffed, but the wall remains. This isn't just for my glory. You are a part of this now.\n"
    sp "Полум'я може бути загашене, але стіна залишається. Це не тільки для моєї слави. Ти тепер теж частина цього.\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:561
