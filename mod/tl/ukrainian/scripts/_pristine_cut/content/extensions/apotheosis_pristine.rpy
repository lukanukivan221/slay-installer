    # voice "audio/_pristine/voice/apotheosis/princess/11.flac"
    voice "audio/_pristine/voice/apotheosis/princess/11.flac"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:543
translate ukrainian apotheosis_contrarian_freedom_join_18e454ed:

    # sp "This world is but an illusion keeping us from my ascension. A play of shadows dancing on the wall.\n"
    sp "Цей світ - ілюзія, що перешкоджає моєму вознесенню. Танець тіней на стіні.\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:545
translate ukrainian apotheosis_contrarian_freedom_join_b0a84bc2:

    # voice "audio/_pristine/voice/apotheosis/paranoid/4.flac"
    # paranoid "Yes... she understands!\n"
    voice "audio/_pristine/voice/apotheosis/paranoid/4.flac"
    paranoid "Так... вона розуміє!\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:556
