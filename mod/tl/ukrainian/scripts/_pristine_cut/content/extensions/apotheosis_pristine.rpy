    # voice "audio/_pristine/voice/apotheosis/narrator/10.flac"
    voice "audio/_pristine/voice/apotheosis/narrator/10.flac"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:182
translate ukrainian contrarian_pristine_apoth_menu_e08a5585:

    # n "You strike, but her skin is like steel. Though you stab at her again and again, the wounds are superficial, mere etchings in a sprawling surface.\n"
    n "Ти завдаєш удару, але її шкіра міцна, як сталь. Ти б'єш клинком знову і знову, але залишаєш на її широких грудях лише неглибокі подряпини.\n"

# game/scripts/_pristine_cut/content/extensions/apotheosis_pristine.rpy:183
translate ukrainian contrarian_pristine_apoth_menu_dfbaa250:

