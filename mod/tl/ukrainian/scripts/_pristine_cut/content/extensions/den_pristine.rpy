    # voice "audio/_pristine/voice/den/hunted/31.flac"
    # hunted "Bad! This is a bad thing to do!\n"
    voice "audio/_pristine/voice/den/hunted/31.flac"
    hunted "Ні! Не роби цього!\n"

# game/scripts/_pristine_cut/content/extensions/den_pristine.rpy:972
