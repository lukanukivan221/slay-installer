    # extend "" nointeract
    extend "" nointeract

# game/scripts/_pristine_cut/content/new/cage.rpy:329
translate ukrainian cage_woods_blade_toss_menu_34b6876b:

    # voice "audio/_pristine/voice/cage/skeptic/28.flac"
    # skeptic "Too bad. This is a mutiny.\n"
    voice "audio/_pristine/voice/cage/skeptic/28.flac"
    skeptic "Дуже погано. Це бунт.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:331
translate ukrainian cage_woods_blade_toss_menu_f5dabd90:

    # voice "audio/_pristine/voice/cage/hero/_extra1a.flac"
    # hero "Hey, you can't just do that! We're not supposed to be able to make our own decisions.\n"
    voice "audio/_pristine/voice/cage/hero/_extra1a.flac"
    hero "Ей, ти не можеш просто так це зробити! Ми не повинні бути в змозі приймати власні рішення.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:334
translate ukrainian cage_woods_blade_toss_menu_f29afc90:

    # voice "audio/_pristine/voice/cage/skeptic/_extra1.flac"
    # skeptic "Yeah? Well I did it last time.\n"
    voice "audio/_pristine/voice/cage/skeptic/_extra1.flac"
    skeptic "Так? Ну, я робив це минулого разу.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:336
translate ukrainian cage_woods_blade_toss_menu_e9ae5e65:

    # voice "audio/_pristine/voice/cage/hero/_extra2b.flac"
    # hero "What if I stop you? Maybe I can make choices too.\n"
    voice "audio/_pristine/voice/cage/hero/_extra2b.flac"
    hero "А якщо я тебе зупиню? Може, я теж можу робити вибір?\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:338
translate ukrainian cage_woods_blade_toss_menu_aa829cac:

    # voice "audio/_pristine/voice/cage/skeptic/_extra2.flac"
    # skeptic "Good luck with that. I've got backup this time, haven't I?\n"
    voice "audio/_pristine/voice/cage/skeptic/_extra2.flac"
    skeptic "Щасти тобі з цим. Цього разу мене прикриють, так?\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:340
translate ukrainian cage_woods_blade_toss_menu_adcf18f7:

    # voice "audio/_pristine/voice/cage/paranoid/_extra1a.flac"
    # paranoid "Yeah! Alliance of convenience! We're not throwing it away. It's important!\n"
    voice "audio/_pristine/voice/cage/paranoid/_extra1a.flac"
    paranoid "Так! Альянс зручності! Ми його не викинемо. Це важливо!\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:343
translate ukrainian cage_woods_blade_toss_menu_94210779:

    # voice "audio/_pristine/voice/cage/hero/_extra3.flac"
    # hero "Hey, what are you— Are you serious? That's no fair, let me go!\n"
    voice "audio/_pristine/voice/cage/hero/_extra3.flac"
    hero "Агов, ви що... Ви серйозно? Це не чесно! Пустіть мене!\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:345
translate ukrainian cage_woods_blade_toss_menu_730a4ee9:

    # voice "audio/_pristine/voice/cage/paranoid/_extra2a.flac"
    # paranoid "Sorry! Has to be done.\n"
    voice "audio/_pristine/voice/cage/paranoid/_extra2a.flac"
    paranoid "Вибач, але так треба.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:360
