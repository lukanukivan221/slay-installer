    # extend "" nointeract
    extend "" nointeract

# game/scripts/_pristine_cut/content/new/cage.rpy:319
translate ukrainian cage_woods_blade_toss_menu_c8de8303:

    # voice "audio/_pristine/voice/cage/skeptic/27.flac"
    # skeptic "Are you even listening to me? Does my opinion count for nothing?! We're never getting out of here if you don't listen! I'm intervening, we're not tossing the blade.\n"
    voice "audio/_pristine/voice/cage/skeptic/27.flac"
    skeptic "Ти мене взагалі слухаєш? Чи моя думка нічого не значить?! Ми ніколи не вийдемо звідси, якщо ти не послухаєш! Я втручаюся, ми не кинемо клинка.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:322
translate ukrainian cage_woods_blade_toss_menu_3236d946:

    # voice "audio/_pristine/voice/cage/broken/9.flac"
    # broken "You don't have to listen to him. He hasn't gotten us anywhere.\n"
    voice "audio/_pristine/voice/cage/broken/9.flac"
    broken "Не треба його слухати. Він нікуди нас не приведе.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:325
