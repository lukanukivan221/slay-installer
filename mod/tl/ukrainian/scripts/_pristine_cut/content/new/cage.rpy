    # voice "audio/_pristine/voice/cage/princess/2.flac"
    # sp "It's not like you're any different.\n"
    voice "audio/_pristine/voice/cage/princess/2.flac"
    sp "Та й ти не особливо змінився.\n"

# game/scripts/_pristine_cut/content/new/cage.rpy:971
