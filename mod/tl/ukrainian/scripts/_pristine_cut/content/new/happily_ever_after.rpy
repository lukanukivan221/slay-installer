# game/scripts/_pristine_cut/content/new/happily_ever_after.rpy:85
translate ukrainian happy_start_460b0159:

    # voice "audio/_pristine/voice/happy/skeptic/4.flac"
    # skeptic "Yeah. A certain lovesick fool has been suspiciously quiet. One might even say {i}absent{/i}.\n"
    voice "audio/_pristine/voice/happy/skeptic/4.flac"
    skeptic "Так. Щось притихла наша жертва кохання. На нього не схоже. Таке відчуття, що {i}він пропав{/i}.\n"

# game/scripts/_pristine_cut/content/new/happily_ever_after.rpy:87
translate ukrainian happy_start_4c19ce41:

    # voice "audio/_pristine/voice/happy/hero/15.flac"
    # hero "Oh. You're right. It's a little quiet here, isn't it? Maybe that's... one of the rules. Maybe only one 'bonus voice' is allowed to be here at a time.\n"
