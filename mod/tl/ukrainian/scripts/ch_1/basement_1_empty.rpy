    # voice "audio/voices/ch1/empty/narrator/empty_n_93.flac"
    # n "Don't waver now.\n"
    voice "audio/voices/ch1/empty/narrator/empty_n_93.flac"
    n "Не вагайся.\n"

# game/scripts/ch_1/basement_1_empty.rpy:1282
