    voice "audio/voices/ch1/empty/narrator/empty_n_97.flac"

# game/scripts/ch_1/basement_1_empty.rpy:1316
translate ukrainian basement_1_empty_rescue_controlled_2_start_dc497604:

    # n "You seize a moment of hesitation to throw her off of you and shakily push yourself back to your knees.\n"
    n "Ти користуєшся моментом вагання, щоб скинути її з себе, і тремтячими рухами ставиш себе назад на коліна."

# game/scripts/ch_1/basement_1_empty.rpy:1317
