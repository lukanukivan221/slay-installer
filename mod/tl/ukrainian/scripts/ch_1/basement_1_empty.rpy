# game/scripts/ch_1/basement_1_empty.rpy:1274
translate ukrainian basement_1_empty_rescue_controlled_2_start_7a328f1f:

    # hero "I thought we had the upper hand, but it's as if she's barely even threatened by us.\n"
    hero "Я думав, що ми маємо перевагу, але таке враження, що ми їй майже не загрожуємо.\n"

# game/scripts/ch_1/basement_1_empty.rpy:1276
translate ukrainian basement_1_empty_rescue_controlled_2_start_3b3ba5ea:

    # voice "audio/voices/ch1/empty/narrator/empty_n_92.flac"
    # n "It's an act. She's wounded and unarmed. There's nothing she can do to hurt you.\n"
    voice "audio/voices/ch1/empty/narrator/empty_n_92.flac"
    n "Це блеф. Вона поранена і беззбройна. Що вона зробить?.\n"

# game/scripts/ch_1/basement_1_empty.rpy:1278
translate ukrainian basement_1_empty_rescue_controlled_2_start_adbdd112:

    # voice "audio/voices/ch1/empty/hero/empty_h_32.flac"
    # hero "I'm not so sure...\n"
    voice "audio/voices/ch1/empty/hero/empty_h_32.flac"
