    # voice "audio/voices/ch1/empty/princess/empty_p_69.flac"
    voice "audio/voices/ch1/empty/princess/empty_p_69.flac"

# game/scripts/ch_1/basement_1_empty.rpy:1271
translate ukrainian basement_1_empty_rescue_controlled_2_start_9bae5b1c:

    # sp "You've made a terrible enemy, and there's nothing in the world that can possibly save you from me.\n"
    sp "Ти нажив собі страшного ворога, і ніщо в світі не врятує тебе від мене.\n"

# game/scripts/ch_1/basement_1_empty.rpy:1272
