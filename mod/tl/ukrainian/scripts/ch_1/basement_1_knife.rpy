    # voice "audio/voices/ch1/knife/narrator/knife_n_60.flac"
    voice "audio/voices/ch1/knife/narrator/knife_n_60.flac"

# game/scripts/ch_1/basement_1_knife.rpy:740
translate ukrainian basement_1_knife_slay_join_1c10e50e:

    # n "Are you {i}serious{/i}?\n"
    n "Ви це серйозно?\n"

# game/scripts/ch_1/basement_1_knife.rpy:741
translate ukrainian basement_1_knife_slay_join_31b20081:

