    # voice "audio/voices/ch1/knife/princess/stab_p_99.flac"
    voice "audio/voices/ch1/knife/princess/stab_p_99.flac"

# game/scripts/ch_1/basement_1_knife.rpy:2428
translate ukrainian basement_1_knife_rescue_controlled_2_start_cd8bad57:

    # sp "You bastard! If I have to kill you to leave this place, I'll {i}do it{/i}.\n"
    sp "Виродок! Якщо мені доведеться тебе вбити, щоб піти звідси, я це {i}зроблю{/i}.\n"

# game/scripts/ch_1/basement_1_knife.rpy:2429
translate ukrainian basement_1_knife_rescue_controlled_2_start_972f321c:

    # voice "audio/voices/ch1/knife/hero/k_h_60a.flac"
    voice "audio/voices/ch1/knife/hero/k_h_60a.flac"

# game/scripts/ch_1/basement_1_knife.rpy:2431
translate ukrainian basement_1_knife_rescue_controlled_2_start_7a328f1f:

    # hero "I thought we had the upper hand, but it's as if she's barely even threatened by us.\n"
    hero "Я думав, що ми маємо перевагу, але таке враження, що ми їй майже не загрожуємо.\n"

# game/scripts/ch_1/basement_1_knife.rpy:2433
translate ukrainian basement_1_knife_rescue_controlled_2_start_139bbd97:

    # voice "audio/voices/ch1/knife/narrator/knife_n_176.flac"
    # n "It's an act. She's unarmed and there's nothing she can do to hurt you.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_176.flac"
    n "Вона прикидається. Вона поранена і беззбройна. Що вона зробить?.\n"

# game/scripts/ch_1/basement_1_knife.rpy:2435
translate ukrainian basement_1_knife_rescue_controlled_2_start_f7688f8e:

    # voice "audio/voices/ch1/knife/hero/k_h_61.flac"
    # hero "I'm not so sure...\n"
    voice "audio/voices/ch1/knife/hero/k_h_61.flac"
    hero "Я не впевнений...\n"

# game/scripts/ch_1/basement_1_knife.rpy:2437
translate ukrainian basement_1_knife_rescue_controlled_2_start_385bf7aa:

    # voice "audio/voices/ch1/knife/narrator/knife_n_177.flac"
    # n "Don't waver now.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_177.flac"
    n "Не вагайся.\n"

# game/scripts/ch_1/basement_1_knife.rpy:2439
translate ukrainian basement_1_knife_rescue_controlled_2_start_8e4cb20d:

    # voice "audio/voices/ch1/knife/narrator/knife_n_177b.flac"
    voice "audio/voices/ch1/knife/narrator/knife_n_177b.flac"

# game/scripts/ch_1/basement_1_knife.rpy:2446
translate ukrainian basement_1_knife_rescue_controlled_2_start_2f07faec:

    # n "As you raise your blade to strike again, she kicks out, knocking your legs out from under you.\n"
    n "Коли ти готуєшся завдати смертельного удару, вона збиває тебе з ніг\n"

# game/scripts/ch_1/basement_1_knife.rpy:2448
translate ukrainian basement_1_knife_rescue_controlled_2_start_e15254a9:

    # voice "audio/voices/ch1/knife/narrator/knife_n_178.flac"
    voice "audio/voices/ch1/knife/narrator/knife_n_178.flac"

# game/scripts/ch_1/basement_1_knife.rpy:2456
translate ukrainian basement_1_knife_rescue_controlled_2_start_4ca96a4f:

    # n "The two of you struggle on the ground. You lash out with the blade, slicing wherever you can. Her fists connect with your body again and again, each blow stronger than the last, shattering bone and rupturing tissue with reckless abandon.\n"
    n "Ви двоє боретеся на землі. Ти наносиш удари, де тільки можеш. Вона б'є знову і знову, кожен удар сильніший за попередній, розбиваючи кістки і розриваючи тканини з безрозсудною недбалістю.\n"

# game/scripts/ch_1/basement_1_knife.rpy:2457
translate ukrainian basement_1_knife_rescue_controlled_2_start_fca4519b:

    # voice "audio/voices/ch1/knife/hero/k_h_62.flac"
    # hero "Forget trying to rescue her. This is about {i}survival{/i} now. Give her everything you've got.\n"
    voice "audio/voices/ch1/knife/hero/k_h_62.flac"
    hero "Забудь про спроби врятувати її. Зараз мова йде про виживання. Порви її!\n"

# game/scripts/ch_1/basement_1_knife.rpy:2460
