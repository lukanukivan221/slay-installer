    # extend "" nointeract
    extend "" nointeract

# game/scripts/ch_1/basement_1_knife.rpy:235
translate ukrainian basement_1_nerves_steeled_menu_3a7ce1c8:

    # voice "audio/voices/ch1/knife/narrator/knife_n_11.flac"
    # n "If you go into this expecting to die, you're going to die.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_11.flac"
    n "Якщо ти йдеш туди, очікуючи смерті, то ти помреш.\n"

# game/scripts/ch_1/basement_1_knife.rpy:238
translate ukrainian basement_1_nerves_steeled_menu_d42a0fa7:

    # voice "audio/voices/ch1/knife/princess/stab_p_18.flac"
    # p "Hesitating? Why don't you drop the knife and the two of us can be civilized with each other.\n"
    voice "audio/voices/ch1/knife/princess/stab_p_18.flac"
