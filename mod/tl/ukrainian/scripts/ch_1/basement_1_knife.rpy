    voice "audio/voices/ch1/knife/narrator/knife_n_0.flac"
    n "Її голос лунає знизу.\n"

# game/scripts/ch_1/basement_1_knife.rpy:27
translate ukrainian basement_1_knife_start_7dd2169e:

    # voice "audio/voices/ch1/knife/princess/stab_p_1.flac"
