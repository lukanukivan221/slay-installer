    # voice "audio/voices/ch1/knife/narrator/knife_n_50.flac"
    voice "audio/voices/ch1/knife/narrator/knife_n_50.flac"

# game/scripts/ch_1/basement_1_knife.rpy:641
translate ukrainian basement_1_knife_slay_join_f0d24987:

    # n "And then she brings her knee to your throat.\n"
    n "А потім вона підносить коліно до твого горла.\n"

# game/scripts/ch_1/basement_1_knife.rpy:643
translate ukrainian basement_1_knife_slay_join_9e895ae9:

    # voice "audio/voices/ch1/knife/narrator/knife_n_51.flac"
    # n "She leans into it with the kind of weight you didn't think her slight frame could possibly possess, shattering your windpipe and leaving you starved for breath.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_51.flac"
    n "Вона давить сильніше, повільно видушуючи залишки твого життя.\n"

# game/scripts/ch_1/basement_1_knife.rpy:645
