    voice "audio/voices/ch1/knife/narrator/knife_n_107.flac"
    n "Їй не обов'язково знати, як знищити світ, щоб бути здатною це зробити.\n"

# game/scripts/ch_1/basement_1_knife.rpy:1460
translate ukrainian basement_1_shared_task_join_5a4f0bac:

    # voice "audio/voices/ch1/knife/narrator/knife_n_108.flac"
