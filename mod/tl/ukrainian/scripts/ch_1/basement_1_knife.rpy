translate ukrainian alt_razor_final_join_903fecf5:

    # voice "audio/voices/ch1/knife/hero/knife_nh_7.flac"
    # hero "For whatever that's worth.\n"
    voice "audio/voices/ch1/knife/hero/knife_nh_7.flac"
    hero "Чого б це не коштувало.\n"

# game/scripts/ch_1/basement_1_knife.rpy:985
translate ukrainian alt_razor_final_join_66b129b1:

    # voice "audio/voices/ch1/knife/narrator/knife_nr_22.flac"
    # n "Everything goes dark, and you die.\n"
    voice "audio/voices/ch1/knife/narrator/knife_nr_22.flac"
    n "Все темніє, і ти помираєш.\n"

# game/scripts/ch_1/basement_1_knife.rpy:992
translate ukrainian alt_razor_final_join_acca7b0a:

    # voice "audio/voices/ch1/knife/narrator/knife_n_71.flac"
    # n "Yes, exactly. It's over.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_71.flac"
    n "Так, саме так. Все скінчено.\n"

# game/scripts/ch_1/basement_1_knife.rpy:999
translate ukrainian alt_razor_final_join_deed7f36:

    # voice "audio/voices/ch1/knife/narrator/knife_n_73.flac"
    # n "It's. Over. Don't get all worked up.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_73.flac"
    n "Все. Кінець. Не хвилюйся.\n"

# game/scripts/ch_1/basement_1_knife.rpy:1007
