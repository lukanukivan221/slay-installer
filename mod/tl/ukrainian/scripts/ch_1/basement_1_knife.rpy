    # voice "audio/voices/ch1/knife/narrator/knife_n_47.flac"
    voice "audio/voices/ch1/knife/narrator/knife_n_47.flac"

# game/scripts/ch_1/basement_1_knife.rpy:623
translate ukrainian basement_1_knife_slay_join_1bf9cfe3:

    # n "You lacked the will to finish the job, your bruised and broken body falling to its knees before her.\n"
    n "Тобі не вистачило волі довести справу до кінця, твоє вкрите синцями і переломами тіло впало перед нею на коліна.\n"

# game/scripts/ch_1/basement_1_knife.rpy:625
translate ukrainian basement_1_knife_slay_join_1734edc3:

    # voice "audio/voices/ch1/knife/narrator/knife_n_48.flac"
    # n "The Princess, exhausted, chest heaving with heavy breaths, tosses the blade away from you.\n"
    voice "audio/voices/ch1/knife/narrator/knife_n_48.flac"
    n "Принцеса, трохи втомлена, відкидає від тебе клинок.\n"

# game/scripts/ch_1/basement_1_knife.rpy:627
translate ukrainian basement_1_knife_slay_join_6a539058:

    # voice "audio/voices/ch1/knife/hero/k_h_21.flac"
