    voice "audio/voices/ch3/fury/princess/17.flac"
    sp "Якщо тобі шкода, то ти будеш стояти і боротися.\n"

# game/scripts/paths/_joint/fury/fury_start.rpy:926
translate ukrainian fury_broken_intro_dc369751:

    # voice "audio/voices/ch3/fury/princess/18.flac"
