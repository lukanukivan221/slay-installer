    voice "audio/voices/ch3/fury/princess/18.flac"
    sp "Мені не потрібен сопливий боягуз. Я хочу вичавити життя з жорстокого серця.\n"

# game/scripts/paths/_joint/fury/fury_start.rpy:933
translate ukrainian fury_broken_intro_8f3afefd:

    # voice "audio/voices/ch3/fury/princess/19.flac"
