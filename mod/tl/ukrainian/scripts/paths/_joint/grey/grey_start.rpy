    # voice "audio/voices/ch3/grey/narrator/51.flac"
    voice "audio/voices/ch3/grey/narrator/51.flac"

# game/scripts/paths/_joint/grey/grey_start.rpy:774
translate ukrainian grey_basement_2b89444a:

    # n "As you descend the final step, the form of the Princess comes into view. A bloated body floating face down in slowly rising waters, her wrist still bound to the wall by a heavy chain.\n"
    n "Коли ти спускаєшся останньою сходинкою, перед тобою постає постать Принцеси. Роздуте тіло плаває обличчям донизу у воді, що повільно піднімається, її зап'ястя все ще прикуте до стіни важким ланцюгом.\n"

# game/scripts/paths/_joint/grey/grey_start.rpy:778
