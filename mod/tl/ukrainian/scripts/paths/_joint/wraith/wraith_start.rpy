translate ukrainian wraith_end_fall_7fc1ca16:

    # voice "audio/voices/ch3/wraith/opportunist/33.flac"
    # opportunist "I thought we already fell forever.\n"
    voice "audio/voices/ch3/wraith/opportunist/33.flac"
    opportunist "Я думав, що ми вже впали остаточно.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1368
translate ukrainian wraith_end_fall_ae7f11fc:

    # voice "audio/voices/ch3/wraith/paranoid/39.flac"
    # paranoid "Yes, but now we're falling with her. She doesn't get an easy way out this time. Just like we didn't get an easy way out last time.\n"
    voice "audio/voices/ch3/wraith/paranoid/39.flac"
    paranoid "Так, але тепер ми падаємо разом з нею. Цього разу у неї не буде легкого виходу. Так само, як і у нас не було легкого виходу минулого разу.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1371
translate ukrainian wraith_end_fall_be1d4bda:

    # voice "audio/voices/ch3/wraith/cheated/36.flac"
    # cheated "What an end. But at least it's ours.\n"
    voice "audio/voices/ch3/wraith/cheated/36.flac"
    cheated "Який кінець. Але принаймні він наш.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1373
translate ukrainian wraith_end_fall_a0ecf627:

    # voice "audio/voices/ch3/wraith/cold/43.flac"
    # cold "An empire of frigid nothing.\n"
    voice "audio/voices/ch3/wraith/cold/43.flac"
    cold "Імперія холодного ніщо.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1376
translate ukrainian wraith_end_fall_0b0b34a9:

    # voice "audio/voices/ch3/wraith/paranoid/40.flac"
    # paranoid "Hahaha! We did it! We actually did it!\n"
    voice "audio/voices/ch3/wraith/paranoid/40.flac"
    paranoid "Ха-ха-ха! Ми зробили це! Ми дійсно зробили це!\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1378
translate ukrainian wraith_end_fall_f72388b2:

    # voice "audio/voices/ch3/wraith/cold/44.flac"
    # cold "Yes, we've really shown them all, haven't we?\n"
    voice "audio/voices/ch3/wraith/cold/44.flac"
    cold "Так, ми дійсно показали їм усім, чи не так?\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1381
translate ukrainian wraith_end_fall_a9bee9b0:

    # voice "audio/voices/ch3/wraith/narrator/69.flac"
    # n "I don't think this qualifies as saving the world, but at least you didn't ruin it.\n"
    voice "audio/voices/ch3/wraith/narrator/69.flac"
    n "Не думаю, що це можна назвати порятунком світу, але принаймні ти його не зруйнував.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1383
