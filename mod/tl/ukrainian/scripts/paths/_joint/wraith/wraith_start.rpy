    voice "audio/voices/ch3/wraith/princess/s40.flac"
    spmid "Я НЕ ДУМАЮ, ЩО ВИ ПРО ЦЕ ДІЗНАЄТЕСЯ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1392
