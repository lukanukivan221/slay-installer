    # extend "" nointeract
    extend "" nointeract

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1070
translate ukrainian wraith_end_free_520ff8a9:

    # voice "audio/voices/ch3/wraith/narrator/59.flac"
    # n "I'd beg you to resist, but you've already surrendered your will to her, haven't you?\n"
    voice "audio/voices/ch3/wraith/narrator/59.flac"
    n "Я благав тебе чинити опір, але ти вже віддав їй свою волю, чи не так?\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1073
translate ukrainian wraith_end_free_72e74fa0:

    # voice "audio/voices/ch3/wraith/paranoid/33.flac"
    # paranoid "You're the puppet master here. If this is the only way we could cut ourselves free from you, so be it.\n"
    voice "audio/voices/ch3/wraith/paranoid/33.flac"
    paranoid "Ти тут лялькар. Якщо це єдиний спосіб звільнитися від тебе, нехай так і буде.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1076
translate ukrainian wraith_end_free_494a1565:

    # voice "audio/voices/ch3/wraith/cheated/31.flac"
    # cheated "Anything to get out of this hellhole.\n"
    voice "audio/voices/ch3/wraith/cheated/31.flac"
    cheated "Що завгодно, аби вибратися з цього пекла.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1079
translate ukrainian wraith_end_free_36d52a9f:

    # voice "audio/voices/ch3/wraith/opportunist/26.flac"
    # opportunist "Yeah. Just like I told you we would. We're here to make things right, and the first step in making things right is admitting when you're wrong.\n"
    voice "audio/voices/ch3/wraith/opportunist/26.flac"
    opportunist "Так. Як я і казав. Ми тут, щоб все виправити, а перший крок до цього - визнати, що ти не правий.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1082
translate ukrainian wraith_end_free_34ef80c6:

    # voice "audio/voices/ch3/wraith/cold/38.flac"
    # cold "It's a new experience. You should try being possessed some time. There's nothing else like it.\n"
    voice "audio/voices/ch3/wraith/cold/38.flac"
    cold "Це новий досвід. Тобі варто спробувати побути одержимим. Це ні з чим не зрівняється.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1084
translate ukrainian wraith_end_free_d0146bfe:

    # voice "audio/voices/ch3/wraith/princess/s19.flac"
    # spmid "THERE'S NOTHING WRONG WITH FIXING YOUR MISTAKES. I'LL REMEMBER THIS. BUT I'LL ALSO NEVER FORGET WHAT YOU'VE DONE.\n"
    voice "audio/voices/ch3/wraith/princess/s19.flac"
    spmid "НЕМАЄ НІЧОГО ПОГАНОГО В ТОМУ, ЩОБ ВИПРАВЛЯТИ СВОЇ ПОМИЛКИ. Я ЦЕ ЗАПАМ'ЯТАЮ. АЛЕ Я ТАКОЖ НІКОЛИ НЕ ЗАБУДУ ТЕ, ЩО ТИ ЗРОБИВ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1090
translate ukrainian wraith_end_free_d8e43dfb:

    # voice "audio/voices/ch3/wraith/narrator/60.flac"
    # n "{i}Sigh.{/i} You toss the last of your will aside and surrender to the Princess.\n"
    voice "audio/voices/ch3/wraith/narrator/60.flac"
    n "{i}Ох.{/i} Ти відкидаєш останню волю і підкоряєшся Принцесі.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1093
