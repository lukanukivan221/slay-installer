    # voice "audio/voices/ch3/wraith/cold/36.flac"
    # cold "Were we?\n"
    voice "audio/voices/ch3/wraith/cold/36.flac"
    cold "Хіба?"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1022
translate ukrainian wraith_possession_fff424ac:

    # voice "audio/voices/ch3/wraith/hero/33.flac"
    # hero "I was!\n"
    voice "audio/voices/ch3/wraith/hero/33.flac"
    hero "Так і є!\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1024
translate ukrainian wraith_possession_9fe692a2:

    # voice "audio/voices/ch3/wraith/princess/s12.flac"
    # spmid "I KNOW YOU WERE. BUT YOU WERE GOING TO DO IT FOR THE WRONG REASONS.\n"
    voice "audio/voices/ch3/wraith/princess/s12.flac"
    spmid "Я ЗНАЮ, ЩО ТАК. АЛЕ ВИ ЗБИРАЛИСЯ ЗРОБИТИ ЦЕ З НЕПРАВИЛЬНИХ ПРИЧИН.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1026
translate ukrainian wraith_possession_4b2326aa:

    # voice "audio/voices/ch3/wraith/hero/34.flac"
    # hero "And what reasons are those supposed to be? Nobody should be able to be judge, jury, and executioner.\n"
    voice "audio/voices/ch3/wraith/hero/34.flac"
    hero "І які це мають бути причини? Ніхто не повинен бути суддею, присяжним і катом.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1029
translate ukrainian wraith_possession_b21efb77:

    # voice "audio/voices/ch3/wraith/princess/s13.flac"
    # spmid "WEREN'T YOU ALL OF THOSE THINGS FOR ME?\n"
    voice "audio/voices/ch3/wraith/princess/s13.flac"
    spmid "ХІБА ТИ НЕ ЗРОБИВ ВСЕ ЦЕ ПО ВІДНОШЕННЮ ДО МЕНЕ?\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1031
translate ukrainian wraith_possession_7b7f1070:

    # voice "audio/voices/ch3/wraith/princess/s14.flac"
    # spmid "I CAN GRASP THE SHAPE OF EVERYTHING YOU THINK AND FEEL. YOU THINK YOU ARE NUMB, BUT YOU ARE HOPELESS AND PARANOID. YOU ARE AFRAID OF ME.\n"
    voice "audio/voices/ch3/wraith/princess/s14.flac"
    spmid "Я МОЖУ ВЛОВИТИ ФОРМУ ВСЬОГО, ЩО ТИ ДУМАЄШ І ВІДЧУВАЄШ. ТИ ДУМАЄШ, ЩО ТИ ЗАЦІПЕНІВ, АЛЕ ТИ БЕЗНАДІЙНИЙ І ПАРАНОЇДАЛЬНИЙ. ТИ БОЇшся МЕНЕ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1033
translate ukrainian wraith_possession_e7d687a5:

    # voice "audio/voices/ch3/wraith/princess/s15.flac"
    # spmid "AND I DON'T FEEL REGRET, EVEN IN THE RECESSES OF YOUR SOUL. THERE IS NOT A DROP OF SYMPATHY TO BE FOUND.\n"
    voice "audio/voices/ch3/wraith/princess/s15.flac"
    spmid "І Я НЕ ВІДЧУВАЮ ЖАЛЮ, НАВІТЬ У НАЙПОТАЄМНІШИХ ЗАКУТКАХ ТВОЄЇ ДУШІ. НЕМАЄ ЖОДНОЇ КРАПЛІ СИМПАТІЇ, ЯКУ МОЖНА БУЛО Б ЗНАЙТИ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1036
translate ukrainian wraith_possession_6dbffe8c:

    # voice "audio/voices/ch3/wraith/hero/35.flac"
    # hero "We weren't always like this, we—\n{w=2.0}{nw}"
    voice "audio/voices/ch3/wraith/hero/35.flac"
    hero "Ми не завжди були такими, ми...\n{w=2.0}{nw}"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1039
translate ukrainian wraith_possession_07b2b596:

    # voice "audio/voices/ch3/wraith/cold/37.flac"
    # cold "I've always been like this.\n"
    voice "audio/voices/ch3/wraith/cold/37.flac"
    cold "Я завжди був таким.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1042
translate ukrainian wraith_reap_join_4875ad47:

    # voice "audio/voices/ch3/wraith/princess/s16.flac"
    # spmid "ALL I'M DOING IS REAPING WHAT YOU'VE SOWN.\n"
    voice "audio/voices/ch3/wraith/princess/s16.flac"
    spmid "ВСЕ, ЩО Я РОБЛЮ, - ЦЕ ПОЖИНАЮ ТЕ, ЩО ТИ ПОСІЯВ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1044
translate ukrainian wraith_reap_join_eb7ffc7c:

    # voice "audio/voices/ch3/wraith/paranoid/32.flac"
    # paranoid "We don't have to let her out. I've been storing up a little bit of will since she forced her way in here. Take it and throw us all into the basement. It's better than giving her what she wants.\n"
    voice "audio/voices/ch3/wraith/paranoid/32.flac"
    paranoid "Ми не повинні її випускати. Я накопичив трохи волі відтоді, як вона увірвалася сюди. Візьми її і кинь нас усіх у підвал. Це краще, ніж дати їй те, що вона хоче.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1047
translate ukrainian wraith_reap_join_c9f92c99:

    # voice "audio/voices/ch3/wraith/princess/s17.flac"
    # spmid "YOU WOULDN'T DARE! MOVE! NOW!\n"
    voice "audio/voices/ch3/wraith/princess/s17.flac"
    spmid "ТИ НЕ ПОСМІЄШ! РУХАЙСЯ! НЕГАЙНО!\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1049
translate ukrainian wraith_reap_join_6ef2be75:

