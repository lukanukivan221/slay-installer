    voice "audio/voices/ch3/wraith/princess/s10.flac"
    spmid "І ЦЕ КАЖЕ ВБИВЦЯ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1015
translate ukrainian wraith_possession_af8fce1d:

    # voice "audio/voices/ch3/wraith/princess/s11.flac"
    # spmid "AND IT IS RUDE TO LOCK SOMEBODY AWAY IN A DARK DARK BASEMENT FOREVER. IT IS RUDE TO LEAVE SOMEBODY TO ROT.\n"
    voice "audio/voices/ch3/wraith/princess/s11.flac"
    spmid "І НЕВВІЧЛИВО ЗАМИКАТИ КОГОСЬ У ТЕМНОМУ ПІДВАЛІ НАЗАВЖДИ. НЕВВІЧЛИВО ЗАЛИШАТИ КОГОСЬ ГНИТИ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1017
