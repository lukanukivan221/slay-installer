    # voice "audio/voices/ch3/wraith/princess/s23.flac"
    # spmid "YES. LET. ME. OUT.\n"
    voice "audio/voices/ch3/wraith/princess/s23.flac"
    spmid "ТАК. ВИПУСТИ. МЕНЕ.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1138
