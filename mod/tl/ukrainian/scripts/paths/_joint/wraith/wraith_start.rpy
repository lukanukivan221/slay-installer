    cold "Досить неввічливо з'являтися в чужому тілі і так ним керувати.\n"

# game/scripts/paths/_joint/wraith/wraith_start.rpy:1012
translate ukrainian wraith_possession_e3f58e71:

    # voice "audio/voices/ch3/wraith/princess/s10.flac"
