    # voice "audio/voices/ch2/adversary/_fight/hero/23.flac"
    # hero "{i}What?!{/i}\n"
    voice "audio/voices/ch2/adversary/_fight/hero/23.flac"
    hero "{i}Що?!{/i}\n"

