# game/scripts/paths/adversary/adversary_1/adversary_1_start.rpy:384
translate ukrainian cabin_interior_2_adversary_menu_f4bbe4a5:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_31.flac"
