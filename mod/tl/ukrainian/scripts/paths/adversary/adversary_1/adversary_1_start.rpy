    # voice "audio/voices/ch2/adversary/narrator/ch2_an_13.flac"
    # n "Her fierce voice carries up the stairs.\n"
    voice "audio/voices/ch2/adversary/narrator/ch2_an_13.flac"
    n "Її лютий голос лунає на сходах.\n"

# game/scripts/paths/adversary/adversary_1/adversary_1_start.rpy:501
translate ukrainian adversary_cabin_1_mirror_join_66c54ffb:

    # voice "audio/voices/ch2/adversary/princess/ch2_ap_1.flac"
