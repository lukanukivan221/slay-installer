    voice "audio/voices/ch3/den/narrator/28.flac"

# game/scripts/paths/beast/beast_2/beast_2.rpy:409
translate ukrainian beast_2_cabin_menu_eb192c5b:

    # n "You reach forward and wave your hand over the open hole leading to the basement.\n"
    n "Ти тягнешся вперед і махаєш рукою над відкритим отвором, що веде до підвалу.\n"

# game/scripts/paths/beast/beast_2/beast_2.rpy:414
