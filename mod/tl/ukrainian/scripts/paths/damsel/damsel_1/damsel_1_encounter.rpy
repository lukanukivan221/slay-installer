# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:1014
translate ukrainian damsel_depersonalization_menu_2b810cdb:

    # voice "audio/voices/ch2/damsel/_basement/hero/37.flac"
    # hero "Yeah... she is, isn't she?\n"
    voice "audio/voices/ch2/damsel/_basement/hero/37.flac"
    hero "Так... так і є, хіба ні?\n"

# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:1016
translate ukrainian damsel_depersonalization_menu_69af4e1a:

    # voice "audio/voices/ch2/damsel/_basement/smitten/41.flac"
    # smitten "Or she's innocent. How could a beauty like her be capable of ending the world? It's not rational!\n"
    voice "audio/voices/ch2/damsel/_basement/smitten/41.flac"
    smitten "Або вона невинна. Як може така красуня, як вона, бути здатною покінчити зі світом? Це не раціонально!\n"

# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:1033
translate ukrainian damsel_depersonalization_menu_9d76a693:

    # voice "audio/voices/mound/bonus/path.flac"
    # mound "This path is already worn by travel and has been seen by one of my many eyes. You cannot walk it again. Change your course.\n"
    voice "audio/voices/mound/bonus/path.flac"
    mound "Ця дорога вже стомлена мандрівками, і її бачило одне з моїх численних очей. Ти не зможеш пройти нею знову. Зміни свій курс.\n"

# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:1035
translate ukrainian damsel_depersonalization_menu_3ed0ec34:

    # voice "audio/voices/ch2/beast/_encounter/hero/31.flac"
    # hero "Wait... what?!\n"
    voice "audio/voices/ch2/beast/_encounter/hero/31.flac"
    hero "Зачекай... що?!\n"

# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:1038
