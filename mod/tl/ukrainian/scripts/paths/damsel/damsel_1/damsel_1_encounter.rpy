    voice "audio/voices/ch2/damsel/_basement/hero/12.flac"
    hero "Як ти думаєш, чи є у неї хтось, подібний до Нього, хто говорить їй, що робити?\n"

# game/scripts/paths/damsel/damsel_1/damsel_1_encounter.rpy:207
translate ukrainian damsel_end_world_join_a2fc5121:

    # voice "audio/voices/ch2/damsel/_basement/narrator/18a.flac"
