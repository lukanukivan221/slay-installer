# game/scripts/paths/nightmare/nightmare_1/nightmare_1_start.rpy:372
translate ukrainian cabin_interior_2_nightmare_menu_e78e9051:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_6.flac"
