    # extend "" nointeract
    extend "" nointeract

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:827
translate ukrainian nightmare_upstairs_slay_menu_9d76a693:

    # voice "audio/voices/mound/bonus/path.flac"
    # mound "This path is already worn by travel and has been seen by one of my many eyes. You cannot walk it again. Change your course.\n"
    voice "audio/voices/mound/bonus/path.flac"
    mound "Ця дорога вже стомлена мандрівками, і її бачило одне з моїх численних очей. Ти не зможеш пройти нею знову. Зміни свій курс.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:837
translate ukrainian nightmare_upstairs_slay_menu_fa5ed3d8:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/45.flac"
    # n "Look at that! It's locked. What a relief for the world.\n"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/45.flac"
    n "Поглянь на це! Зачинено. Яке полегшення для світу.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:844
translate ukrainian nightmare_upstairs_slay_menu_9d76a693_1:

    # voice "audio/voices/mound/bonus/path.flac"
    # mound "This path is already worn by travel and has been seen by one of my many eyes. You cannot walk it again. Change your course.\n"
    voice "audio/voices/mound/bonus/path.flac"
    mound "Ця дорога вже стомлена мандрівками, і її бачило одне з моїх численних очей. Ти не зможеш пройти нею знову. Зміни свій курс.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:849
translate ukrainian nightmare_upstairs_slay_menu_4a101b5a:

    # voice "audio/voices/ch2/nightmare/_encounter/princess/74.flac"
    voice "audio/voices/ch2/nightmare/_encounter/princess/74.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:863
translate ukrainian nightmare_upstairs_slay_menu_afde9393:

    # sp "Come along, now. You don't want to be stuck down here.\n"
    sp "Ходімо. Ти ж не хочеш тут застрягти.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:864
translate ukrainian nightmare_upstairs_slay_menu_4c68c051:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/46.flac"
    # n "The Princess glides onto the stairs and beckons you to follow. If only you had a weapon. This would be the perfect opportunity to use it.\n"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/46.flac"
    n "Принцеса ковзає по сходах і кличе тебе за собою. Якби у тебе була зброя. Це була б чудова нагода нею скористатися.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:866
translate ukrainian nightmare_upstairs_slay_menu_7c82fbd3:

    # voice "audio/voices/ch2/nightmare/_encounter/hero/29.flac"
    # hero "But we don't. And we've already made our choice.\n"
    voice "audio/voices/ch2/nightmare/_encounter/hero/29.flac"
    hero "Але ми не маємо. І ми вже зробили свій вибір.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:870
translate ukrainian nightmare_upstairs_slay_menu_ea639495:

    # voice "audio/voices/ch2/nightmare/_encounter/paranoid/27.flac"
    # paranoid "We can still grab it on the way out.\n"
    voice "audio/voices/ch2/nightmare/_encounter/paranoid/27.flac"
    paranoid "Ми все ще можемо захопити її по дорозі.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:872
translate ukrainian nightmare_upstairs_slay_menu_9acc0b2f:

    # voice "audio/voices/ch2/nightmare/_encounter/hero/30.flac"
    # hero "You're not wrong, but maybe you should let me handle the suggestions for the time being. Just focus on keeping us alive.\n"
    voice "audio/voices/ch2/nightmare/_encounter/hero/30.flac"
    hero "Ти не помиляєшся, але, можливо, тобі варто дозволити мені поки що займатися пропозиціями. Зосередься на тому, щоб зберегти нам життя.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:874
translate ukrainian nightmare_upstairs_slay_menu_5676e17b:

    # voice "audio/voices/ch2/nightmare/_encounter/paranoid/28.flac"
    # paranoid "Haha. Right.\n"
    voice "audio/voices/ch2/nightmare/_encounter/paranoid/28.flac"
    paranoid "Ха-ха. Так.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:877
translate ukrainian nightmare_upstairs_slay_menu_faf4ec7f:

    # paranoid "Heart. Lungs. Liver. Nerves. Heart...\n"
    paranoid "Серце. Легені. Печінка. Нерви. Серце...\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:879
translate ukrainian nightmare_upstairs_slay_menu_11fdb519_1:

    # extend "" nointeract
    extend "" nointeract

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:882
translate ukrainian nightmare_upstairs_slay_menu_28687706:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/47.flac"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/47.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:886
translate ukrainian nightmare_upstairs_slay_menu_c6179a65:

    # n "You step onto the basement stairs, following the Princess as she ascends. Pins and needles punctuate your every step, an uncomfortable and constant reminder of your mortality.\n"
    n "Ти ступаєш на сходи до підвалу, слідуючи за Принцесою, яка піднімається вгору. Поколювання пронизують кожен твій крок, незручне і постійне нагадування про твою смертність.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:887
translate ukrainian nightmare_upstairs_slay_menu_ee01a077:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/48.flac"
    # n "Is this really worth it?\n"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/48.flac"
    n "Чи справді це того варте?\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:889
translate ukrainian nightmare_upstairs_slay_menu_ff9a5afc:

    # voice "audio/voices/ch2/nightmare/_encounter/hero/31.flac"
    # hero "At least it's a choice.\n"
    voice "audio/voices/ch2/nightmare/_encounter/hero/31.flac"
    hero "Принаймні, це вибір.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:891
translate ukrainian nightmare_upstairs_slay_menu_10935578:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/49.flac"
    # n "That doesn't justify anything. A terrible choice is still terrible.\n"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/49.flac"
    n "Це нічого не виправдовує. Жахливий вибір все одно залишається жахливим.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:893
translate ukrainian nightmare_upstairs_slay_menu_5d28b98b:

    # voice "audio/voices/ch2/nightmare/_encounter/princess/75.flac"
    # sp "Make sure you keep up. We wouldn't want you to fall.\n"
    voice "audio/voices/ch2/nightmare/_encounter/princess/75.flac"
    sp "Не відставай. Ми не хочемо, щоб ти впав.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:895
translate ukrainian nightmare_upstairs_slay_menu_b7e10e1e:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/50.flac"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/50.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:900
translate ukrainian nightmare_upstairs_slay_menu_cff10fec:

    # n "As if acting on instinct, you crane your neck and look behind you. There is nothing there.\n"
    n "Немов на інстинкті, витягуєш шию і озираєшся назад. Там нічого немає.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:902
translate ukrainian nightmare_upstairs_slay_menu_4a3ba583:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/51.flac"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/51.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:920
translate ukrainian nightmare_upstairs_slay_menu_9c36820b:

    # n "The Princess reaches the top of the stairs and taps the door.\n"
    n "Принцеса досягає вершини сходів і стукає у двері.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:921
translate ukrainian nightmare_upstairs_slay_menu_d90f8ac6:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/51a.flac"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/51a.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:927
translate ukrainian nightmare_upstairs_slay_menu_1ca7e65a:

    # n "Her head twists back in your direction, watching you with dead eyes as the way out creaks open.\n"
    n "Її голова повертається у твій бік, дивлячись мертвими очима, коли вихід зі скрипом відчиняється.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:928
translate ukrainian nightmare_upstairs_slay_menu_2ca844c5:

    # voice "audio/voices/ch2/nightmare/_encounter/narrator/52.flac"
    # n "Don't forget that your pristine blade is still on the other side. You can still make this right. You can still save everyone, and you can still save yourself from becoming her tormented pet.\n"
    voice "audio/voices/ch2/nightmare/_encounter/narrator/52.flac"
    n "Не забувай, що твій бездоганний клинок все ще на іншій стороні. Ти можеш все виправити. Ти все ще можеш врятувати всіх, і ти все ще можеш врятувати себе від того, щоб стати її замученим домашнім улюбленцем.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:932
translate ukrainian nightmare_upstairs_slay_menu_7f0f2f27:

    # voice "audio/voices/ch2/nightmare/_encounter/paranoid/29.flac"
    voice "audio/voices/ch2/nightmare/_encounter/paranoid/29.flac"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:935
translate ukrainian nightmare_upstairs_slay_menu_3082e9d9:

    # paranoid "There's no harm in us having a weapon. All it does is give us more options. And we are dangerously low on those right now.\n"
    paranoid "Немає нічого поганого в тому, що ми маємо зброю. Все, що вона робить, це дає нам більше можливостей. А їх у нас зараз небезпечно мало.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:936
translate ukrainian nightmare_upstairs_slay_menu_76b90a88:

    # voice "audio/voices/ch2/nightmare/_encounter/hero/32.flac"
    # hero "Uh...\n"
    voice "audio/voices/ch2/nightmare/_encounter/hero/32.flac"
    hero "Ах...\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:938
translate ukrainian nightmare_upstairs_slay_menu_1d2aba9c:

    # voice "audio/voices/ch2/nightmare/_encounter/paranoid/30.flac"
    # paranoid "Have you considered that maybe you could operate the autonomic nervous system for a bit? Everyone is trying to manipulate us and I can only do so many things at once to keep us alive.\n"
    voice "audio/voices/ch2/nightmare/_encounter/paranoid/30.flac"
    paranoid "Ти не думав про те, що, можливо, ти міг би трохи попрацювати з вегетативною нервовою системою? Всі намагаються нами маніпулювати, а я мушу робити стільки речей одночасно, щоб ми жили.\n"

# game/scripts/paths/nightmare/nightmare_1/nightmare_encounter.rpy:940
translate ukrainian nightmare_upstairs_slay_menu_5940a0ae:

    # voice "audio/voices/ch2/nightmare/_encounter/hero/33.flac"
    # hero "Er... heart? Erm. Liver?\n"
    voice "audio/voices/ch2/nightmare/_encounter/hero/33.flac"
    hero "Е... серце? Емм. Печінка?\n"

