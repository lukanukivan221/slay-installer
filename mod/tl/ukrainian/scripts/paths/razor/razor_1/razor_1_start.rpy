    # extend "" nointeract
    extend "" nointeract

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:327
translate ukrainian cabin_interior_2_razor_menu_6c619b27:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_26.flac"
    # n "That's because there isn't a mirror. There's a table, the blade sitting on the table, and the door to the basement. There's nothing else in here.\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_26.flac"
    n "Тому що там немає дзеркала. Є стіл, клинок, що лежить на столі, і двері до підвалу. Більше тут нічого немає.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:329
translate ukrainian cabin_interior_2_razor_menu_ea3700a0:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_4.flac"
    # hero "There's definitely a mirror.\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_4.flac"
    hero "Тут точно є дзеркало.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:331
translate ukrainian cabin_interior_2_razor_menu_68e5903f:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_27.flac"
    # n "There isn't.\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_27.flac"
    n "Ні, немає.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:333
translate ukrainian cabin_interior_2_razor_menu_1e9fc1be:

    # voice "audio/voices/ch2/razor/cheated/12.flac"
    # cheated "Can you two stop arguing? It's stressful enough in here without all of this extra noise.\n"
    voice "audio/voices/ch2/razor/cheated/12.flac"
    cheated "Ви можете припинити сперечатися? Тут напружена обстановка і без цього зайвого шуму.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:336
translate ukrainian cabin_interior_2_razor_menu_11fdb519_1:

    # extend "" nointeract
    extend "" nointeract

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:339
translate ukrainian cabin_interior_2_razor_menu_d1e990c4:

    # voice "audio/voices/ch2/adversary/hero/ch2_ah_5.flac"
    # hero "As do I.\n"
    voice "audio/voices/ch2/adversary/hero/ch2_ah_5.flac"
    hero "Як і я.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:342
translate ukrainian cabin_interior_2_razor_menu_b5818ce4:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_30.flac"
    # n "I'm not lying to you. Use your eyes, there is no mirror. Why would I lie about something so meaningless? What good would a mirror even do? Let you waste time preening yourself instead of doing what needs to be done?\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_30.flac"
    n "Я не брешу. Поглянь, тут немає дзеркала. Навіщо мені брехати про щось таке безглузде? Яка користь від дзеркала? Дозволити тобі витрачати час на чепуріння замість того, щоб робити те, що потрібно?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:346
translate ukrainian cabin_interior_2_razor_menu_ecfa802c:

    # voice "audio/voices/ch2/adversary/hero/ch2_ah_6.flac"
    # hero "I care less about that and more about whether we're being lied to. If He's willing to lie about something as innocuous as a mirror, what else is He hiding from us?\n"
    voice "audio/voices/ch2/adversary/hero/ch2_ah_6.flac"
    hero "Мене менше турбує це, а більше те, чи не брешуть нам. Якщо Він готовий брехати про щось таке невинне, як дзеркало, то що ще Він приховує від нас?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:348
translate ukrainian cabin_interior_2_razor_menu_e6a54e42:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_29.flac"
    # n "I'm not lying to you. Use your eyes, there is no mirror. Why would I lie about something so meaningless? What good would it even do?\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_29.flac"
    n "Я не брешу. Поглянь, тут немає дзеркала. Навіщо мені брехати про щось таке безглузде? Яка з цього користь?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:353
translate ukrainian cabin_interior_2_razor_menu_cd73853c:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_5.flac"
    # hero "But it {i}does{/i} matter! Don't you care if we're being lied to? If He's willing to lie about something as innocuous as a mirror, what else is He hiding from us?\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_5.flac"
    hero "Але це не має значення! Хіба тебе не хвилює, що нас обманюють? Якщо Він готовий брехати про щось таке невинне, як дзеркало, то що ще Він приховує від нас?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:355
translate ukrainian cabin_interior_2_razor_menu_b6b38896:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_30.flac"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_30.flac"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:357
translate ukrainian cabin_interior_2_razor_menu_8e5d8049:

    # n "I'm not lying to you. Use your eyes, there is no mirror. Why would I lie about something so meaningless? What good would a mirror even do? Let you waste time preening yourself instead of doing what needs to be done?\n"
    n "Я не брешу. Поглянь, тут немає дзеркала. Навіщо мені брехати про щось таке безглузде? Яка користь від дзеркала? Дозволити тобі витрачати час на чепуріння замість того, щоб робити те, що потрібно?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:358
translate ukrainian cabin_interior_2_razor_menu_e78e9051:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_6.flac"
    # hero "But there {i}was{/i} a mirror a second ago.\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_6.flac"
    hero "Але ж секунду тому тут було дзеркало.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:360
translate ukrainian cabin_interior_2_razor_menu_c7e19eb4:

    # voice "audio/voices/ch2/razor/cheated/13.flac"
    # cheated "And now it's gone. Yet another thing in here playing tricks on us. I hate this place.\n"
    voice "audio/voices/ch2/razor/cheated/13.flac"
    cheated "А тепер його немає. Ще одна річ тут грає з нами злий жарт. Ненавиджу це місце.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:364
translate ukrainian cabin_interior_2_razor_menu_b17b67c2:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_7b.flac"
    # hero "I care about whether we're being lied to. If He's willing to lie about something as innocuous as a mirror, what else could He hiding from us?\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_7b.flac"
    hero "Мене хвилює, чи не брешуть нам. Якщо Він готовий збрехати про таку невинну річ, як дзеркало, то що ще Він приховує від нас?\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:366
translate ukrainian cabin_interior_2_razor_menu_f4bbe4a5:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_31.flac"
    # n "I'm not lying to you, I {i}promise{/i}. There isn't a mirror. Really.\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_31.flac"
    n "Я не брешу тобі, {i}обіцяю{/i}. Дзеркала немає. Справді.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:382
translate ukrainian razor_cabin_1_mirror_join_3de9698b:

    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_32.flac"
    # n "You walk up to the wall next to the basement door. It's a wall. There isn't much to see here.\n"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_32.flac"
    n "Ти підходиш до стіни біля дверей до підвалу. Це стіна. Тут немає на що дивитися.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:385
translate ukrainian razor_cabin_1_mirror_join_35d4d923:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_8.flac"
    # hero "What are you talking about? This isn't a wall. It's a mirror. Or at least it'll {i}be{/i} a mirror once we wipe off that layer of grime.\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_8.flac"
    hero "Про що ти говориш? Це не стіна. Це дзеркало. Або, принаймні, стане дзеркалом, коли ми зітремо цей шар бруду.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:388
translate ukrainian razor_cabin_1_mirror_join_3171c430:

    # voice "audio/voices/ch2/shared/hero/ch2_share_h_9.flac"
    # hero "This really isn't funny.\n"
    voice "audio/voices/ch2/shared/hero/ch2_share_h_9.flac"
    hero "Це дійсно не смішно.\n"

# game/scripts/paths/razor/razor_1/razor_1_start.rpy:392
translate ukrainian razor_cabin_1_mirror_join_11fdb519:

    # extend "" nointeract
    extend "" nointeract

