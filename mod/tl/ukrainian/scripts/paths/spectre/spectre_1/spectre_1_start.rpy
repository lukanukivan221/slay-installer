    # voice "audio/voices/ch2/shared/narrator/ch2_share_n_36.flac"
    voice "audio/voices/ch2/shared/narrator/ch2_share_n_36.flac"

# game/scripts/paths/spectre/spectre_1/spectre_1_start.rpy:430
translate ukrainian spectre_cabin_1_mirror_join_e5176ba8:

    # n "You take the blade from the table. It would be difficult to slay the Princess and save the world without a weapon.\n"
    n "Ти береш клинок зі столу. Без зброї буде важко вбити Принцесу і врятувати світ.\n"

# game/scripts/paths/spectre/spectre_1/spectre_1_start.rpy:453
