    # hero "A g-g-g-ghost!\n"
    hero "П-п-п-привид!\n"

# game/scripts/paths/spectre/spectre_1/spectre_1_start.rpy:500
translate ukrainian spectre_cabin_1_mirror_join_345daea0:

    # voice "audio/voices/ch2/spectre/cold/ch2_cold_16.flac"
    # cold "Oh. Wow. How absolutely terrifying. What's a ghost supposed to do to us?\n"
    voice "audio/voices/ch2/spectre/cold/ch2_cold_16.flac"
    cold "Ого. Вау. Як жахливо. І що ж привид з нами зробить?\n"

# game/scripts/paths/spectre/spectre_1/spectre_1_start.rpy:503
translate ukrainian spectre_cabin_1_mirror_join_e3128c88:

    # voice "audio/voices/ch2/spectre/princess/ch2_sp_1.flac"
    # p "Oh. It's you. Hiya, killer. I was hoping to see you again. I have some issues with how our last meeting went.\n"
    voice "audio/voices/ch2/spectre/princess/ch2_sp_1.flac"
    p "Ох. Це ти. Привіт, вбивце. Я сподівалася побачити тебе знову. У мене є деякі зауваження до того, як пройшла наша остання зустріч.\n"

