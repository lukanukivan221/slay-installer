    # voice "audio/voices/ch2/spectre/_encounter/cold/38.flac"
    # cold "Obviously she's nowhere to be seen.\n"
    voice "audio/voices/ch2/spectre/_encounter/cold/38.flac"
    cold "Звичайно, її ніде не видно.\n"

# game/scripts/paths/spectre/spectre_1/spectre_encounter.rpy:1241
