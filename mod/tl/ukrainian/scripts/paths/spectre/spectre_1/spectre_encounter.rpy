    # voice "audio/voices/ch2/spectre/_encounter/princess/105a.flac"
    voice "audio/voices/ch2/spectre/_encounter/princess/105a.flac"

# game/scripts/paths/spectre/spectre_1/spectre_encounter.rpy:1243
