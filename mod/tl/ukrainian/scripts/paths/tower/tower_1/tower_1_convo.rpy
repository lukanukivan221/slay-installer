    # voice "audio/voices/ch2/tower/_cont/princess/p18.flac"
    voice "audio/voices/ch2/tower/_cont/princess/p18.flac"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:233
translate ukrainian tower_1_join_me_menu_b439e6a2:

    # p "I can. Easily. But that isn't what I want to do.\n"
    p "Я можу. Легко. Але це не те, що я хочу.\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:234
