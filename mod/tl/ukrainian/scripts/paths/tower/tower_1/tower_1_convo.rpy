# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:192
translate ukrainian tower_1_motive_shared_join_638dcab1:

    # p "Meekness. Despair. But a hint at something more. A small taste of stubborn resistance.\n"
    p "Смиренність. Відчай. Але натяк на щось більше. Невеликий присмак впертого опору.\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:193
translate ukrainian tower_1_motive_shared_join_41de8ca1:

    # voice "audio/voices/ch2/tower/_cont/princess/sp5.flac"
    # sp "A grain in the storm of my cosmic wind. Enough to be felt, but not enough to matter.\n"
    voice "audio/voices/ch2/tower/_cont/princess/sp5.flac"
    sp "Піщинка в бурі мого космічного вітру. Достатньо, щоб відчути, але недостатньо, щоб мати значення.\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:196
