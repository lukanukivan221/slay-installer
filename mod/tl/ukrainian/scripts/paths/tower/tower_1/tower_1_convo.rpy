translate ukrainian tower_1_motive_shared_join_d46a009a:

    # p "Confused. Meek. A chorus of submissive indecision. Is that why you came to me unarmed?\n"
    p "Розгублений. Покірний. Хор смиренної нерішучості. Тому ти прийшов до мене беззбройним?\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:186
translate ukrainian tower_1_motive_shared_join_bed3b681:

    # voice "audio/voices/ch2/tower/_cont/princess/p14.flac"
    # p "It doesn't matter. All of it is easily brushed aside.\n"
    voice "audio/voices/ch2/tower/_cont/princess/p14.flac"
    p "Це не має значення. Все легко можна відкинути.\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:189
