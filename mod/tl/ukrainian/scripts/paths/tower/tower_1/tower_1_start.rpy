translate ukrainian tower_cabin_1_mirror_join_3a0bffa9:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_34.flac"
    # n "Your legs buckle, and your knees hit the floor.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_34.flac"
    n "Ноги підкошуються, а коліна боляче вдаряються об підлогу.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:662
translate ukrainian tower_cabin_1_mirror_join_0dc6b4b3:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_35.flac"
    # n "But the words don't leave your mouth. Instead, your legs buckle, and your knees hit the floor.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_35.flac"
    n "Але слова не злітають з твоїх вуст. Натомість ноги підкошуються, а коліна боляче вдаряються об підлогу.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:673
translate ukrainian tower_cabin_1_mirror_join_292dff8e:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_36.flac"
    # n "On her command, you fall to the floor, knees painfully connecting with hard stone.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_36.flac"
    n "За її командою ти падаєш на підлогу, коліна боляче з'єднуються з твердим каменем.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:677
translate ukrainian tower_cabin_1_mirror_join_2729a2eb:

    # voice "audio/voices/ch2/tower/princess/ch2_tp_8.flac"
    # p "That's my good little bird. Now... why don't we talk?\n"
    voice "audio/voices/ch2/tower/princess/ch2_tp_8.flac"
    p "Моя хороша маленька пташка. А тепер... чому б нам не поговорити?\n"

