# game/scripts/paths/tower/tower_1/tower_1_start.rpy:587
translate ukrainian tower_cabin_1_mirror_join_e236344e:

    # voice "audio/voices/ch2/tower/hero/ch2_th_14.flac"
    # hero "We didn't have to do that.\n"
    voice "audio/voices/ch2/tower/hero/ch2_th_14.flac"
    hero "Ми не повинні були цього робити.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:589
translate ukrainian tower_cabin_1_mirror_join_8294f1dc:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_29.flac"
    # n "And yet you did.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_29.flac"
    n "І все ж таки ти це зробив.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:591
translate ukrainian tower_cabin_1_mirror_join_14ea1fe1:

    # voice "audio/voices/ch2/tower/broken/ch2_broken_24.flac"
    # broken "She's so much more than us. You wouldn't understand what it feels like to be in her presence.\n"
    voice "audio/voices/ch2/tower/broken/ch2_broken_24.flac"
    broken "Вона набагато більша за нас. Тобі не зрозуміти, як це - бути в її присутності.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:600
translate ukrainian tower_cabin_1_mirror_join_c392b8ef:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_30.flac"
    # n "As if on command, the blade slips from your grasp. It clatters uselessly to the floor.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_30.flac"
    n "Ніби за командою, клинок вислизає з рук. Воно безглуздо брязкає об підлогу.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:602
translate ukrainian tower_cabin_1_mirror_join_d3972c75:

    # voice "audio/voices/ch2/tower/hero/ch2_th_15.flac"
    # hero "But we {i}didn't{/i} drop it. We decided to grip it tighter, remember? Are you really just going to let that happen to us?\n"
    voice "audio/voices/ch2/tower/hero/ch2_th_15.flac"
    hero "Але ми {i}не{/i} впустили його. Ми вирішили тримати його міцніше, пам'ятаєш? Ти справді дозволиш цьому статися з нами?\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:604
translate ukrainian tower_cabin_1_mirror_join_c7b9ea54:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_31.flac"
    # n "I have a duty to report facts as facts, and the fact is that you dropped the blade.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_31.flac"
    n "Я зобов'язаний повідомляти факти як факти, і факт полягає в тому, що ти впустив клинок.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:606
translate ukrainian tower_cabin_1_mirror_join_a7ad97d4:

    # voice "audio/voices/ch2/tower/broken/ch2_broken_25.flac"
    # broken "Of course we dropped it. She's so much more than us. You wouldn't understand what it feels like to be in her presence.\n"
    voice "audio/voices/ch2/tower/broken/ch2_broken_25.flac"
    broken "Звісно, ми його впустили. Вона набагато більша за нас. Тобі не зрозуміти, що ми відчуваємо, перебуваючи в її присутності.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:609
translate ukrainian tower_cabin_1_mirror_join_4792c430:

    # voice "audio/voices/ch2/tower/narrator/ch2_tn_32.flac"
    # n "Oh, I understand what's going on, and you'd better snap yourself out of it.\n"
    voice "audio/voices/ch2/tower/narrator/ch2_tn_32.flac"
    n "О, я розумію, що відбувається, і вам краще прийти до тями.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:614
translate ukrainian tower_cabin_1_mirror_join_47bca14b:

    # voice "audio/voices/ch2/tower/princess/ch2_tp_5.flac"
    # p "I see your hands are empty. You've already given up, haven't you? You aren't even going to try and kill me. How sweet.\n"
    voice "audio/voices/ch2/tower/princess/ch2_tp_5.flac"
    p "Я бачу, порожні руки. Ти вже здався, чи не так? Ти навіть не спробуєш мене вбити. Як мило.\n"

# game/scripts/paths/tower/tower_1/tower_1_start.rpy:617
translate ukrainian tower_cabin_1_mirror_join_d6129684:

    # voice "audio/voices/ch2/tower/princess/ch2_tp_5a.flac"
    # p "And more than a little disappointing.\n"
    voice "audio/voices/ch2/tower/princess/ch2_tp_5a.flac"
    p "І більш ніж трохи розчаровує.\n"

