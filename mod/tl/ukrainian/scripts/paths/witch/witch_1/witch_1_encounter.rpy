    # voice "audio/voices/ch2/witch/_encounter/hero/49.flac"
    # hero "Have you considered not being like that?\n"
    voice "audio/voices/ch2/witch/_encounter/hero/49.flac"
    hero "А ти не замислювався над тим, щоб не бути таким?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1565
