    # voice "audio/voices/ch2/witch/_encounter/opportunist/82.flac"
    voice "audio/voices/ch2/witch/_encounter/opportunist/82.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1798
translate ukrainian witch_locked_joke_196cdd9a:

    # opportunist "I think He's gone...\n"
    opportunist "Думаю, Його більше немає...\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1799
translate ukrainian witch_locked_joke_e44e0b82:

    # voice "audio/voices/ch2/witch/_encounter/princess/95.flac"
    voice "audio/voices/ch2/witch/_encounter/princess/95.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1802
translate ukrainian witch_locked_joke_957f9e14:

    # spmid "Something strange is twisting this place out of shape.\n"
    spmid "Щось дивне викривляє це місце.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1803
translate ukrainian witch_locked_joke_8b7bb68d:

    # voice "audio/voices/ch2/witch/_encounter/princess/96.flac"
    voice "audio/voices/ch2/witch/_encounter/princess/96.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1806
translate ukrainian witch_locked_joke_621f5e69:

    # spmid "I wasn't feeling much of anything. But now I feel... cold.\n"
    spmid "Я нічого не відчувала. Але зараз, мене пронизує... холод.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1843
translate ukrainian witch_locked_joke_f09226e7:

    # truth "But you don't get the chance to respond, nor will you ever. It's time to leave. Memory returns.\n"
    truth "Але ти не маєш можливості відповісти, і ніколи не матимеш. Пора йти. Пам'ять повертається.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1845
translate ukrainian witch_locked_joke_b5225fdd:

    # truth "But you don't get the chance to respond. Something has taken her away, and it's left something else in her place.\n"
    truth "Але ти не маєш можливості відповісти. Щось забрало її, а на її місці залишило щось інше.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1859
