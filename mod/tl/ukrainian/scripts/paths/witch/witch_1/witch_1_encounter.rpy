    # voice "audio/voices/ch2/witch/_encounter/princess/93.flac"
    voice "audio/voices/ch2/witch/_encounter/princess/93.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1768
translate ukrainian witch_locked_joke_750cdb8a:

    # spmid "Seems I'm not the only one, though.\n"
    spmid "Здається, я не одна така.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1770
translate ukrainian witch_locked_joke_199754c8:

    # voice "audio/voices/ch2/witch/_encounter/hero/57.flac"
    voice "audio/voices/ch2/witch/_encounter/hero/57.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1773
translate ukrainian witch_locked_joke_52a43349:

    # hero "Wait... are we both just stuck here?\n"
    hero "Зачекай... ми що, обидва тут застрягли?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1774
translate ukrainian witch_locked_joke_58d6d844:

    # voice "audio/voices/ch2/witch/_encounter/narrator/106.flac"
    # n "It certainly seems that way. What a way to go. But look on the bright side! In all this chaos, you may have actually managed to deliver a lethal wound. The world could be saved after all.\n"
    voice "audio/voices/ch2/witch/_encounter/narrator/106.flac"
    n "Здається, що так. Що ж, так і буде. Але подивіться на світлу сторону! У всьому цьому хаосі тобі, можливо, вдалося вилікувати смертельне поранення. Зрештою, світ може бути врятований.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1776
translate ukrainian witch_locked_joke_da2ca920:

    # voice "audio/voices/ch2/witch/_encounter/princess/94.flac"
    voice "audio/voices/ch2/witch/_encounter/princess/94.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1783
translate ukrainian witch_locked_joke_bc6c0d74:

    # spmid "We're going to die together. Isn't that a lovely treat! I wonder how long it'll take. Maybe I'll get to watch the worms lick your bones clean.\n"
    spmid "Ми помремо разом. Хіба це не чудово? Цікаво, скільки часу це займе. Може, я побачу, як черв'яки вилизуватимуть твої кістки дочиста.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1784
translate ukrainian witch_locked_joke_345d377a:

    # voice "audio/voices/ch2/witch/_encounter/hero/58.flac"
    voice "audio/voices/ch2/witch/_encounter/hero/58.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1789
translate ukrainian witch_locked_joke_dcba3a06:

    # hero "Is that really what's going to happen now?\n"
    hero "Невже це дійсно те, що зараз станеться?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1790
translate ukrainian witch_locked_joke_1fc3309f:

    # voice "audio/voices/ch2/witch/_encounter/hero/59.flac"
    voice "audio/voices/ch2/witch/_encounter/hero/59.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1793
translate ukrainian witch_locked_joke_46163bf2:

    # hero "... Hello? Are you still there? What happens next?\n"
    hero "... Алло? Ти ще там? Що буде далі?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1794
translate ukrainian witch_locked_joke_94c3854d:

    # truth "He doesn't respond.\n"
    truth "Він не відповідає.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1795
