    # voice "audio/voices/ch2/witch/_encounter/narrator/105.flac"
    voice "audio/voices/ch2/witch/_encounter/narrator/105.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1764
translate ukrainian witch_locked_joke_6c8f0a74:

    # n "She eyes you with intensity, silence falling between you for a long moment as her gaze travels up and down your battered form.\n"
    n "Вона дивиться на тебе пильно, між вами на довгу мить западає тиша, поки її погляд мандрує вгору і вниз по твоїй побитій фігурі.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1765
