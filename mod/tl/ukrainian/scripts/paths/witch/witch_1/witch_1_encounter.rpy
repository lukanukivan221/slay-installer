    # voice "audio/voices/ch2/witch/_encounter/hero/11.flac"
    # hero "I... uh... abstain?\n"
    voice "audio/voices/ch2/witch/_encounter/hero/11.flac"
    hero "Я... е-е... утримаюсь?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:337
translate ukrainian witch_blade_given_join_730a0ba3:

    # voice "audio/voices/ch2/witch/_encounter/opportunist/15.flac"
    # opportunist "You abstain?! She's going to kill us if we give it to her!\n"
    voice "audio/voices/ch2/witch/_encounter/opportunist/15.flac"
    opportunist "Ти утримуєшся?! Вона вб'є нас, якщо ми їй це дамо!\n"

