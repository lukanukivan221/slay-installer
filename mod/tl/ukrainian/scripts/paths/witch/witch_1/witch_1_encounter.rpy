    # voice "audio/voices/ch2/witch/_encounter/princess/92.flac"
    voice "audio/voices/ch2/witch/_encounter/princess/92.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1760
translate ukrainian witch_locked_joke_e11d426a:

    # spmid "Something inside me is... broken. Isn't that a cruel twist of the knife? Isn't that so, so funny?\n"
    spmid "Щось всередині мене... розбите. Хіба це не іронічно? Хіба це не смішно?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1761
