# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1704
translate ukrainian witch_locked_joke_9cc7772e:

    # voice "audio/voices/ch2/witch/_encounter/opportunist/71.flac"
    # opportunist "But we gave it our all! No regrets! Besides, she's got to have it as bad as us, right?\n"
    voice "audio/voices/ch2/witch/_encounter/opportunist/71.flac"
    opportunist "Але ми виклалися на повну! Не шкодую! До того ж, їй має бути так само погано, як і нам, чи не так?\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:1710
