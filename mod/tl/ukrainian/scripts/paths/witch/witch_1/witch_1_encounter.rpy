    # voice "audio/voices/ch2/witch/_encounter/narrator/15.flac"
    voice "audio/voices/ch2/witch/_encounter/narrator/15.flac"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:330
translate ukrainian witch_blade_given_join_a7018b0b:

    # n "You — you can't be serious.\n"
    n "Ти - ти не можеш бути серйозним.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:331
translate ukrainian witch_blade_given_join_99566779:

    # voice "audio/voices/ch2/witch/_encounter/opportunist/14.flac"
    # opportunist "Now hold on, we should put this to a vote. That blade is one of our most valuable assets. We can't just give it to her! What if she uses it to kill us? I vote no.\n"
    voice "audio/voices/ch2/witch/_encounter/opportunist/14.flac"
    opportunist "Зачекайте, ми повинні поставити це на голосування. цей клинок - одне з наших найцінніших надбань. Ми не можемо просто віддати його їй! Що, як вона використає його, щоб убити нас? Я голосую проти.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:333
translate ukrainian witch_blade_given_join_6000c5ba:

    # voice "audio/voices/ch2/witch/_encounter/narrator/16.flac"
    # n "As do I.\n"
    voice "audio/voices/ch2/witch/_encounter/narrator/16.flac"
    n "Як і я.\n"

# game/scripts/paths/witch/witch_1/witch_1_encounter.rpy:335
