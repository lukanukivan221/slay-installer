    contrarian "Це схоже на нас, еге ж?\n"

# game/scripts/special/felina/felina_cabin_strange.rpy:634
translate ukrainian felina_cabin_strange_talk_blade_menu_1612659b:

    # voice "audio/voices/felina/cabin/stranger/hero/28.flac"
    # hero "Yeah. We really are the same.\n"
    voice "audio/voices/felina/cabin/stranger/hero/28.flac"
    hero "Так. Ми дійсно однакові.\n"

# game/scripts/special/felina/felina_cabin_strange.rpy:641
