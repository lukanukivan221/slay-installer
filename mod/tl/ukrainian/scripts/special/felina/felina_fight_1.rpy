# game/scripts/special/felina/felina_fight_1.rpy:428
translate ukrainian felina_lecture_join_ab2355bf:

    # mounds "And I will not stop our dance until you understand.\n"
    mounds "І я не зупиню наш танець, поки ти не зрозумієш.\n"

# game/scripts/special/felina/felina_fight_1.rpy:431
