    # voice "audio/voices/felina/fight/wild6.flac"
    voice "audio/voices/felina/fight/wild6.flac"

# game/scripts/special/felina/felina_fight_1.rpy:4653
translate ukrainian felina_wild_menu_cc273f93:

    # mound "Then leave with me, and we will see it all together.\n"
    mound "Тоді підемо зі мною, і ми побачимо все разом.\n"

