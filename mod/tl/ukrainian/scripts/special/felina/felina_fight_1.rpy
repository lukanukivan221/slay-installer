# game/scripts/special/felina/felina_fight_1.rpy:1381
translate ukrainian felina_den_menu_e3e453d9:

    # mound "That, too, is part of the dance. Would you rob yourself of the ability to choose?\n"
    mound "Це теж частина танцю. Хіба ти відбереш у себе можливість вибору?\n"

# game/scripts/special/felina/felina_fight_1.rpy:1384
translate ukrainian felina_den_menu_f3af0726:

    # voice "audio/_pristine/voice/_climax/felina/_temp/27.flac"
