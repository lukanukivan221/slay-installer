    # mound "You doused the flames of false devotion, and in my despair you lifted my chin, and the two of us danced beneath the stars.\n"
    mound "Побачивши мій відчай, ти погасив полум'я помилкового прагнення і звернув мій погляд наверх, щоб ми станцювали під зірками.\n"

# game/scripts/special/felina/felina_fight_1.rpy:5410
translate ukrainian felina_happy_start_838e825e:

    # voice "audio/_pristine/voice/happy/mound/6.flac"
