    # voice "audio/voices/felina/fight/grey2.flac"
    voice "audio/voices/felina/fight/grey2.flac"

# game/scripts/special/felina/felina_fight_1.rpy:4386
translate ukrainian felina_grey_start_1efeb35a:

    # mounds "Hollow eyes watch from the dry corners of a memory. A home built on all the futures that were supposed to be, preserved until the moment of reunion. The fire of the heart sets it all ablaze.\n"
    mounds "Порожні очі дивляться з сухих куточків пам'яті. Дім, побудований на всіх майбутніх, які повинні були бути, збережений до моменту возз'єднання. Вогонь серця підпалює все це.\n"

# game/scripts/special/felina/felina_fight_1.rpy:4388
translate ukrainian felina_grey_start_3adadd13:

    # voice "audio/voices/felina/fight/grey3.flac"
    voice "audio/voices/felina/fight/grey3.flac"

# game/scripts/special/felina/felina_fight_1.rpy:4400
translate ukrainian felina_grey_start_c677de4b:

    # mounds "Hollow eyes watch from the dark corners of a forgotten place flooded by emotions left unspoken. The tide rises.\n"
    mounds "Порожні очі дивляться з темних кутків забутого місця, переповнені невисловленими емоціями. Приплив піднімається.\n"

# game/scripts/special/felina/felina_fight_1.rpy:4401
translate ukrainian felina_grey_start_5da64cdc:

    # voice "audio/voices/felina/fight/grey4.flac"
    voice "audio/voices/felina/fight/grey4.flac"

