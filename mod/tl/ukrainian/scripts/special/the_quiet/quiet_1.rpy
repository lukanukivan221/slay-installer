    voice "audio/voices/mound/mound1/63.flac"

# game/scripts/special/the_quiet/quiet_1.rpy:564
translate ukrainian quiet1_decision_bf8779be:

    # mound "How can I know? I am flickers in something sprawling and unilluminated.\n"
    mound "Звідки мені знати? Я мерехчу в чомусь розлогому і неосвітленому.\n"

